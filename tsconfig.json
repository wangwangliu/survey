{
    "compilerOptions": {
      "esModuleInterop": true,
      "noEmit": true,
      "pretty": true,
      "module": "commonjs",
      "skipLibCheck": true,
      "target": "es2017",
      "baseUrl": "./",
      "paths": {
        "client/*": ["./client/*"]
      },
      "allowSyntheticDefaultImports": true,
      "declaration": true,
      "strictNullChecks": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "jsx": "react",
      "noUnusedParameters": true,
      "noUnusedLocals": true,
      "noImplicitAny": false,
      "lib": ["dom", "es7"],
    },
    "exclude": ["node_modules", "build", "scripts"],
    "include":["**/*.tsx","typed-css.d.ts"],
  }